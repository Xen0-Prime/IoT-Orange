    {
        "id": "0387ed7b5f43547e",
        "type": "function",
        "z": "343da118b2120754",
        "name": "Extract Position",
        "func": "const data = msg.payload;\nlet position = data.v;\n\n// Debug: log le type et la valeur brute\nnode.warn(\"Position v type: \" + typeof position);\nnode.warn(\"Position v value: \" + JSON.stringify(position));\n\n// Si c'est une string, parser le JSON\nif (typeof position === 'string') {\n    try {\n        position = JSON.parse(position);\n        node.warn(\"Parsed position: \" + JSON.stringify(position));\n    } catch (e) {\n        node.error(\"Failed to parse position: \" + e.message);\n        return null;\n    }\n}\n\nconst x = position.x || position.X || 0;\nconst y = position.y || position.Y || 0;\nconst z = position.z || position.Z || 0;\n\n// Message pour les gauges X, Y, Z\nconst msgX = { payload: x, topic: \"position_x\" };\nconst msgY = { payload: y, topic: \"position_y\" };\nconst msgZ = { payload: z, topic: \"position_z\" };\n\n// Message pour debug\nconst msgDebug = {\n    payload: { x, y, z },\n    topic: \"position\"\n};\n\nreturn [msgX, msgY, msgZ, msgDebug];",
